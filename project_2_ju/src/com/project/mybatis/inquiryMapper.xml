<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="inquiry">

	<select id="ione" parameterType="int" resultType="com.mystudy.model.vo.inquiryVO">
			SELECT *
			FROM INQUIRY
			WHERE QA_NO = #{qaNo}
	</select>
	
</mapper>